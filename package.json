    "mocha": "~4.0.1",
    "node-sass": "^4.7.2",
    "prop-types": "^15.5.10",
    "raw-loader": "^0.5.1",
    "react": "^16.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2",
    "sass-loader": "^6.0.6",
    "sinon": "~4.0.1",
    "storybook-readme": "^3.2.0",
    "style-loader": "^0.19.1",
    "trash": "~4.1.0",
    "unexpected": "~10.36.0",
