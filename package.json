{
  "name": "@salocreative/alerts",
  "version": "0.0.1",
  "description": "Alerts for react. Uses context to manage alerts",
  "main": "./dist/modules/index.js",
  "scripts": {
    "bundle-dist": "webpack -p --config webpack.js",
    "bundle-modules": "rm -rf dist/modules/ && babel --copy-files src --out-dir dist/modules",
    "bundle-watch": "babel --copy-files src --watch --out-dir dist/modules",
    "bundle": "npm run bundle-dist && npm run bundle-modules",
    "demo": "start-storybook -p 6002 -s ./public",
    "lint": "eslint ./src",
    "test": "npm run lint && mocha -R spec --compilers jsx:test/compiler.js ./src/*Test.jsx",
    "release": "chmod +x release.sh && ./release.sh",
    "storybook": "start-storybook -p 6002 -s ./public",
    "build-storybook": "build-storybook -s ./public -o .out",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "author": "Salo Creative",
  "repository": {
    "type": "git",
    "url": "https://github.com/SaloCreative/alerts"
  },
  "license": "MIT",
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.6",
    "@storybook/addon-info": "^3.3.6",
    "@storybook/addon-knobs": "^3.3.6",
    "@storybook/addon-links": "^3.3.6",
    "@storybook/addon-options": "^3.3.6",
    "@storybook/cli": "^3.3.6",
    "@storybook/react": "^3.3.6",
    "@storybook/storybook-deployer": "^2.2.0",
    "babel-cli": "6.26.0",
    "babel-core": "~6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "~7.1.2",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "^6.24.1",
    "classnames": "~2.2.5",
    "css-loader": "^0.28.7",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
